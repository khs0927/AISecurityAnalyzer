# 빌드/개발에서 사용되는 환경 변수 팸플릿
# 실제 배포에서는 시크릿으로 관리됩니다

# 세션 보안
SESSION_SECRET=your_session_secret_here

# OpenAI API (ADK Python Agent로 대체 예정)
OPENAI_API_KEY=your_openai_api_key_here
USE_ADK_PYTHON_AGENT=true

# 데이터베이스 (Neon)
DATABASE_URL=postgresql://user:password@host:port/database
PGDATABASE=neondb
PGHOST=your_pg_host_here
PGPORT=5432
PGUSER=your_pg_user_here
PGPASSWORD=your_pg_password_here

# Firebase (VITE_ 접두사는 클라이언트에서 접근 가능)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_APP_ID=your_firebase_app_id_here
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
FIREBASE_DATABASE_URL=your_firebase_db_url_here

# API 키
PUBMED_API_KEY=your_pubmed_api_key_here
GITHUB_TOKEN=your_github_token_here
HF_TOKEN=your_hf_token_here
HUGGINGFACE_TOKEN=your_huggingface_token_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
KAKAO_MAP_API_KEY=your_kakao_map_api_key_here

# Vercel 배포
VERCEL_TOKEN=your_vercel_token_here
VERCEL_ORG_ID=your_vercel_org_id_here
VERCEL_PROJECT_ID=your_vercel_project_id_here

# 환경 설정
APP_ENV=production
DEBUG=false
PORT=3000
HOST=localhost
NODE_ENV=development
LOG_FILE=./logs/app.log
MODEL_CHECK_INTERVAL=300000 