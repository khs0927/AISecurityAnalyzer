# 서버리스 배포 환경 변수

# API 서버 설정
API_PORT=8080
NODE_ENV=production

# 프론트엔드 설정
VITE_API_URL=https://api.nottoday-medical.xyz
VITE_PUSHER_APP_KEY=your-pusher-app-key
VITE_PUSHER_CLUSTER=your-pusher-cluster

# Supabase 설정
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_PUBLIC_KEY=your-public-key
SUPABASE_SERVICE_KEY=your-service-key

# Pusher 설정
PUSHER_APP_ID=your-pusher-app-id
PUSHER_APP_KEY=your-pusher-app-key
PUSHER_APP_SECRET=your-pusher-app-secret
PUSHER_CLUSTER=your-pusher-cluster

# AI 모델 설정
MODEL_PATH=/home/oracle/nottoday-server/models
USE_CPU_ONLY=true
BATCH_SIZE=1

# 스마트워치 설정
SMARTWATCH_API_KEY=your-smartwatch-api-key
NOTIFICATION_SERVICE_URL=https://notifications.nottoday-medical.xyz

# 보안 설정
JWT_SECRET=your-jwt-secret
ENCRYPTION_KEY=your-encryption-key

# 모바일 푸시 알림
EXPO_ACCESS_TOKEN=your_expo_token

# 클라우드 스토리지 (선택 사항)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret 