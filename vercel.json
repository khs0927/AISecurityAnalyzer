{
  "version": 2,
  "installCommand": "NPM_CONFIG_PRODUCTION=false pnpm i --frozen-lockfile",
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/public",
  "framework": null,
  "env": {},
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "regions": ["iad1"]
} 